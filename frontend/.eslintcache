\";\r\nimport MessageBox from \"../components/MessageBox\";\r\nimport { useDispatch, useSelector } from \"react-redux\";\r\nimport { listProducts } from \"../actions/productActions\";\r\n\r\nexport default function HomeScreen() {\r\n    const dispatch = useDispatch();\r\n    const productList = useSelector((state) => state.productList);\r\n    const { loading, error, products } = productList;\r\n\r\n    useEffect(() => {\r\n        dispatch(listProducts());\r\n    }, []);\r\n    return (\r\n        <div>\r\n            {loading ? (\r\n                <LoadingBox></LoadingBox>\r\n            ) : error ? (\r\n                <MessageBox variant=\"danger\">{error}</MessageBox>\r\n            ) : (\r\n                <div className=\"row center\">\r\n                    {products.map((product) => {\r\n                        return <Product key={product._id} product={product} />;\r\n                    })}\r\n                </div>\r\n            )}\r\n        </div>\r\n    );\r\n}\r\n","C:\\Users\\tonyr\\Documents\\Projects\\hanalei_designs\\frontend\\src\\components\\LoadingBox.jsx",[],"C:\\Users\\tonyr\\Documents\\Projects\\hanalei_designs\\frontend\\src\\components\\MessageBox.jsx",[],"C:\\Users\\tonyr\\Documents\\Projects\\hanalei_designs\\frontend\\src\\store.js",["106"],"C:\\Users\\tonyr\\Documents\\Projects\\hanalei_designs\\frontend\\src\\reducers\\productReducers.js",[],"C:\\Users\\tonyr\\Documents\\Projects\\hanalei_designs\\frontend\\src\\constants\\productConstants.js",[],"C:\\Users\\tonyr\\Documents\\Projects\\hanalei_designs\\frontend\\src\\actions\\productActions.js",[],"C:\\Users\\tonyr\\Documents\\Projects\\hanalei_designs\\frontend\\src\\screens\\CartScreen.jsx",["107"],"import React, { useEffect } from \"react\";\r\nimport { useDispatch, useSelector } from \"react-redux\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { addToCart, removeFromCart } from \"../actions/cartActions\";\r\nimport MessageBox from \"../components/MessageBox\";\r\n\r\nexport default function CartScreen(props) {\r\n    const dispatch = useDispatch();\r\n    const productId = props.match.params.id;\r\n    const qty = props.location.search ? Number(props.location.search.split(\"=\")[1]) : 1;\r\n    const cart = useSelector((state) => state.cart);\r\n    const { cartItems } = cart;\r\n\r\n    const removeFromCartHandler = (id) => {\r\n        console.log(\"CLICKED\", id);\r\n        dispatch(removeFromCart(id));\r\n    };\r\n    const checkoutHandler = () => {\r\n        props.history.push(\"/signin?redirect=shipping\");\r\n    };\r\n\r\n    useEffect(() => {\r\n        if (productId) {\r\n            dispatch(addToCart(productId, qty));\r\n        }\r\n    }, []);\r\n\r\n    console.log(\"cart items\", cartItems);\r\n\r\n    return (\r\n        <div className=\"row top\">\r\n            <div className=\"col-2\">\r\n                <h1>Shopping Cart</h1>\r\n                {cartItems.length === 0 ? (\r\n                    <MessageBox>\r\n                        Cart is Empty. <Link to=\"/\">Shop Now</Link>\r\n                    </MessageBox>\r\n                ) : (\r\n                    <ul>\r\n                        {cartItems.map((item) => {\r\n                            return (\r\n                                <li key={item.product}>\r\n                                    <div className=\"row\">\r\n                                        <div>\r\n                                            <img\r\n                                                src={item.image}\r\n                                                alt={item.name}\r\n                                                className=\"small\"\r\n                                            />\r\n                                        </div>\r\n                                        <div className=\"min-30\">\r\n                                            <Link to={`/product/${item.product}`}>{item.name}</Link>\r\n                                        </div>\r\n                                        <div>\r\n                                            <select\r\n                                                value={item.qty}\r\n                                                onChange={(e) =>\r\n                                                    dispatch(\r\n                                                        addToCart(\r\n                                                            item.product,\r\n                                                            Number(e.target.value)\r\n                                                        )\r\n                                                    )\r\n                                                }\r\n                                            >\r\n                                                {[...Array(item.countInStock).keys()].map((x) => (\r\n                                                    <option key={x + 1} value={x + 1}>\r\n                                                        {x + 1}\r\n                                                    </option>\r\n                                                ))}\r\n                                            </select>\r\n                                        </div>\r\n                                        <div>${item.price}</div>\r\n                                        <div>\r\n                                            <button\r\n                                                type=\"button\"\r\n                                                onClick={() => removeFromCartHandler(item.product)}\r\n                                            >\r\n                                                Delete\r\n                                            </button>\r\n                                        </div>\r\n                                    </div>\r\n                                </li>\r\n                            );\r\n                        })}\r\n                    </ul>\r\n                )}\r\