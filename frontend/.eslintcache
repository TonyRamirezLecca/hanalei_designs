 <img\r\n                                                src={item.image}\r\n                                                alt={item.name}\r\n                                                className=\"small\"\r\n                                            />\r\n                                        </div>\r\n                                        <div className=\"min-30\">\r\n                                            <Link to={`/product/${item.product}`}>{item.name}</Link>\r\n                                        </div>\r\n                                        <div>\r\n                                            <select\r\n                                                value={item.qty}\r\n                                                onChange={(e) =>\r\n                                                    dispatch(\r\n                                                        addToCart(\r\n                                                            item.product,\r\n                                                            Number(e.target.value)\r\n                                                        )\r\n                                                    )\r\n                                                }\r\n                                            >\r\n                                                {[...Array(item.countInStock).keys()].map((x) => (\r\n                                                    <option key={x + 1} value={x + 1}>\r\n                                                        {x + 1}\r\n                                                    </option>\r\n                                                ))}\r\n                                            </select>\r\n                                        </div>\r\n                                        <div>${item.price}</div>\r\n                                        <div>\r\n                                            <button\r\n                                             